@*/Admin/IncreseSession*@

<script type="text/javascript">
    function IncreseSessionTime() {
        // 

        var Id = "";
        $.ajax({

            url: '/Admin/IncreseSession',

            dataType: "json",

            type: "POST",

            contentType: 'application/json; charset=utf-8',

            data: JSON.stringify({ Id: Id }),

            async: true,

            processData: false,

            cache: false,

            success: FillSession,

            error: function (xhr) {

            }

        });

    }
    function FillSession(result) {
        
        if (result == "1") {
            location.reload(true);
            $('#myModal').modal('hide');
        }
        else {
            location.reload();
        }


    }
</script>